{
  "manifest_version": 2,
  "name": "Display URL in Console",
  "version": "1.0",
  "permissions": ["activeTab", "https://jsonplaceholder.typicode.com", "https://youtube-to-snipd.pandibay.com/"],
  "host_permissions": ["https://jsonplaceholder.typicode.com", "https://youtube-to-snipd.pandibay.com/"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Execute Code on Button Click"
  }
}
